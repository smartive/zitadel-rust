{
  "branches": [
    "main",
    {
      "name": "next",
      "prerelease": "prerelease"
    }
  ],
  "plugins": [
    "@semantic-release/commit-analyzer",
    "@semantic-release/release-notes-generator",
    "semantic-release-cargo",
    [
      "@semantic-release/github",
      {
        "assets": [
          {
            "path": "target/package/*.crate"
          }
        ]
      }
    ]
  ]
}
